{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "noob/noob-block",
  "version": "1.0.0",
  "title": "Noob block",
  "category": "block",
  "icon": "shortcode",
  "description": "Container block.",
  "supports": { "html": false },
  "editorScript": ["swiper-js","file:./index.js"],
  "script": ["swiper-js"],
  "style": ["custom-swiper-css", "swiper-css"],
  "viewStyle": ["custom-swiper-css", "swiper-css"],
  "attributes": {
    "uniqueId": {
      "type": "string",
      "default": ""
    },
    "sliderType": {
      "type": "string",
      "default": "all"
    },
    "directionVertical": {
      "type": "boolean",
      "default": false
    },
    "numberSlides": {
      "type": "integer",
      "default": 3
    }
  }
}